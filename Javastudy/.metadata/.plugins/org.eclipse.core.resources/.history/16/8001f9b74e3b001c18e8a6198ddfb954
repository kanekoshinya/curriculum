package test2;
import test3.Constants;

public class Check {

	public static void main(String[] args) {

		//Q1 test1インスタンスを生成(苗字と名前は１のものを使うこと)
		Pet test1 = new Pet(Constants.firstName1,Constants.lastName1);

		//Q2 test2インスタンスを生成(苗字と名前は２のものを使うこと)
		Pet test2 = new Pet(Constants.firstName2,Constants.lastName2);

		//Q3 test1とtest2の2人の名前を表示
        System.out.println("2人の名前は"+test1.getFirstName()+test1.getLastName()+"と"+test2.getFirstName()+test2.getLastName()+"です");

        //Q5 上記で作成したtest1インスタンスを利用してPet.javaのQ4 ageメソッド動かすこと(※　先にQ4の問題を解くこと)
        test1.age(test1.getFirstName(),test1.getLastName());

        //Q6 上記で作成したtest2インスタンスを利用してPet.javaのQ4 ageメソッド動かすこと(※　先にQ4の問題を解くこと)
        test2.age(test2.getFirstName(),test2.getLastName());

        //Q7 Pet.javaのQ4 ageメソッドの「値は無効です」と出力されるようにageメソッドを動かすこと(※　先にQ4の問題を解くこと)
	}
}
